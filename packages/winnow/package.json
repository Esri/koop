{
  "name": "winnow",
  "version": "4.1.0",
  "description": "Apply sql-like filters to GeoJSON",
  "main": "src/index.js",
  "scripts": {
    "clean": "shx rm -rf ../../customdata/framework/winnow && shx rm package-lock.json",
    "build": "shx mkdir ../../customdata/framework/winnow && shx cp package.json ../../customdata/framework/winnow && shx cp -r ./src ../../customdata/framework/winnow && shx cp -r ./node_modules ../../customdata/framework/winnow"
  },
  "keywords": [
    "geojson",
    "filter",
    "stream",
    "sql",
    "geoservices",
    "query"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@esri/proj-codes": "^3.1.0",
    "@terraformer/arcgis": "^2.1.1",
    "@terraformer/spatial": "^2.1.2",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/centroid": "^6.0.0",
    "@types/geojson": "^7946.0.7",
    "alasql": "^4.0.0",
    "classybrew": "0.0.3",
    "joi": "^17.6.3",
    "js-sql-parser": "^1.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ngeohash": "^0.6.3",
    "proj4": "^2.9.0",
    "simple-statistics": "^7.0.0",
    "string-hash": "^1.1.3",
    "wkt-parser": "^1.2.2"
  },
  "optionalDependencies": {
    "farmhash": "^3.1.0"
  }
}
