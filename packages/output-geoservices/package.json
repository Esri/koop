{
  "name": "@koopjs/output-geoservices",
  "version": "6.0.2",
  "description": "Wraps FeatureServer as a Koop output plugin",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "nyc -r=json-summary jest && npm run cov:badge",
    "test:cov:unit": "nyc -r=json-summary --report-dir=coverage_unit --temp-dir=.nyc_output_unit jest && npm run cov:badge:unit",
    "cov:badge": "coverage-badges-cli --output ./coverage.svg",
    "cov:badge:unit": "coverage-badges-cli --source coverage_unit/coverage-summary.json --output ./coverage-unit.svg"  
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop.git"
  },
  "keywords": [
    "koop",
    "featureserver",
    "plugin",
    "output",
    "geoservices"
  ],
  "author": "Daniel Fenton <dfenton@esri.com>",
  "license": "Apache-2.0",
  "files": [
    "src/**/*.js",
    "index.d.ts",
    "!**/*.spec.js"
  ],
  "bugs": {
    "url": "https://github.com/koopjs/koop/issues"
  },
  "homepage": "https://github.com/koopjs/koop#readme",
  "dependencies": {
    "@koopjs/featureserver": "7.1.1",
    "@koopjs/logger": "5.0.0"
  }
}
