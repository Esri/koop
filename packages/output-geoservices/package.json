{
  "name": "@koopjs/output-geoservices",
  "version": "8.1.0",
  "description": "Wraps FeatureServer as a Koop output plugin",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:cov:unit": "jest --coverage && npm run cov:badge",
    "test:cov": "jest --coverage && npm run cov:badge",
    "cov:badge": "coverage-badges --output ./coverage.svg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop.git"
  },
  "keywords": [
    "koop",
    "featureserver",
    "plugin",
    "output",
    "geoservices"
  ],
  "author": "Daniel Fenton <dfenton@esri.com>",
  "license": "Apache-2.0",
  "files": [
    "src/**/*.js",
    "index.d.ts",
    "!**/*.spec.js"
  ],
  "bugs": {
    "url": "https://github.com/koopjs/koop/issues"
  },
  "homepage": "https://github.com/koopjs/koop#readme",
  "dependencies": {
    "@koopjs/featureserver": "8.2.0",
    "@koopjs/logger": "5.0.0"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "json",
      "text",
      "lcov"
    ]
  }
}
